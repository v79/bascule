{{#partial "content" }}
	<h2>List of pages</h2>

	<ol>
		{{#each pages }}
			<li><a href="/page/edit/{{refName}}">{{ refName}} - {{title}}</a></li>
		{{/each}}
	</ol>

	<div>
		<h3>Actions</h3>
		<ul>
			<li><a href="/pageTemplate/new">New Page Template</a></li>
			<li><a href="page/new">New Page</a></li>
		</ul>
	</div>
{{/partial}}

{{> base/base}}